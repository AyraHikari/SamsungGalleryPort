<!--
This XML file declares which signature|privileged permissions should be granted to privileged
applications that come with the platform
-->
<permissions>
    <privapp-permissions package="me.ayra.android.providers.media">
        <permission name="android.permission.MANAGE_USERS"/>
        <permission name="android.permission.WRITE_MEDIA_STORAGE"/>
        <permission name="android.permission.WATCH_APPOPS" />
        <permission name="android.permission.SET_PROCESS_LIMIT"/>
        <permission name="com.samsung.android.permission.SEM_MEDIA_CONTENTS"/>
        <!-- Permissions required for reading and logging compat changes -->
        <permission name="android.permission.LOG_COMPAT_CHANGE"/>
        <permission name="android.permission.READ_COMPAT_CHANGE_CONFIG"/>
        <!-- Permissions for reading and writing from cmh provider -->
        <permission name="com.samsung.cmh.data.READ"/>
        <permission name="com.samsung.cmh.data.WRITE"/>
    </privapp-permissions>
</permissions>
